<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="chart_background">#202326</color>
    <color name="chart_green">#DF392E</color>
    <color name="chart_grid_line">#CC353941</color>
    <color name="chart_ma10">#FE5EDF</color>
    <color name="chart_ma20">#E7A520</color>
    <color name="chart_ma5">#18CCF6</color>
    <color name="chart_red">#10A589</color>
    <color name="chart_selector">#c830343C</color>
    <color name="chart_tab_background">#30343C</color>
    <color name="chart_tab_indicator">#FF6601</color>
    <color name="chart_text">#80B1B2B6</color>
    <color name="chart_white">#fff</color>
    <color name="colorAccent">#FF4081</color>
    <color name="colorPrimary">#3F51B5</color>
    <color name="colorPrimaryDark">#303F9F</color>
    <dimen name="chart_bottom_padding">15dp</dimen>
    <dimen name="chart_candle_line_width">1dp</dimen>
    <dimen name="chart_candle_width">4dp</dimen>
    <dimen name="chart_grid_line_width">1dp</dimen>
    <dimen name="chart_line_width">0.5dp</dimen>
    <dimen name="chart_point_width">6dp</dimen>
    <dimen name="chart_selector_text_size">12sp</dimen>
    <dimen name="chart_text_size">10sp</dimen>
    <dimen name="chart_top_padding">15dp</dimen>
    <string name="app_name">KChartLib</string>
    <string name="kchart_string1">高:</string>
    <string name="kchart_string2">低:</string>
    <string name="kchart_string3">开:</string>
    <string name="kchart_string4">收:</string>
    <string name="kchart_string5">涨跌额:</string>
    <string name="kchart_string6">涨跌幅:</string>
    <string name="kchart_string7">成交量:</string>
    <declare-styleable name="KChartView">
        <!--公共的-->
        <attr format="dimension" name="kc_text_size"/>
        <attr format="color" name="kc_text_color"/>
        <attr format="dimension" name="kc_line_width"/>
        <attr format="color" name="kc_background_color"/>
        <attr format="color" name="kc_selected_line_color"/>
        <attr format="dimension" name="kc_selected_line_width"/>
        <attr format="dimension" name="kc_grid_line_width"/><!--边框线的宽度-->
        <attr format="color" name="kc_grid_line_color"/><!--边框线的颜色-->
        <attr format="dimension" name="kc_point_width"/><!--每格的宽度-->
        <!--macd-->
        <attr format="dimension" name="kc_macd_width"/>
        <attr format="color" name="kc_dif_color"/>
        <attr format="color" name="kc_dea_color"/>
        <attr format="color" name="kc_macd_color"/>
        <!--kdj-->
        <attr format="color" name="kc_k_color"/>
        <attr format="color" name="kc_d_color"/>
        <attr format="color" name="kc_j_color"/>
        <!--rsi-->
        <attr format="color" name="kc_rsi1_color"/>
        <attr format="color" name="kc_rsi2_color"/>
        <attr format="color" name="kc_ris3_color"/>
        <!--boll-->
        <attr format="color" name="kc_up_color"/>
        <attr format="color" name="kc_mb_color"/>
        <attr format="color" name="kc_dn_color"/>
        <!--main-->
        <attr format="color" name="kc_ma5_color"/>
        <attr format="color" name="kc_ma10_color"/>
        <attr format="color" name="kc_ma20_color"/>
        <attr format="dimension" name="kc_candle_width"/><!--蜡烛图宽度--><!--下面子图的宽度在代码里面设置-->
        <attr format="dimension" name="kc_candle_line_width"/><!--蜡烛中间线的宽度-->
        <attr format="color" name="kc_selector_background_color"/>
        <attr format="dimension" name="kc_selector_text_size"/>
        <attr format="boolean" name="kc_candle_solid"/>
        <!--tab-->
        <attr format="color" name="kc_tab_background_color"/>
        <attr format="color" name="kc_tab_text_color"/>
        <attr format="color" name="kc_tab_indicator_color"/>
    </declare-styleable>
</resources>