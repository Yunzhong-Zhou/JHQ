                app:bsb_max="5"
                app:bsb_min="1"
                app:bsb_progress="1"
                app:bsb_second_track_color="#A794FD"
                app:bsb_second_track_size="2dp"
                app:bsb_thumb_color="#FDDD04"
                app:bsb_track_color="@color/white"
                app:bsb_track_size="2dp" />

